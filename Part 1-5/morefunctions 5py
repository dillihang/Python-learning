def greatest_number(num1 : int, num2 : int, num3 : int):
    
    if num1>num2 and num1>num3:
        return num1
    elif num2>num1 and num2>num3:
        return num2
    else:
        return num3
    

print(greatest_number(3, 1, 4)) # 4
print(greatest_number(7, -4, 99)) # 99
print(greatest_number(0, 0, 0)) # 0

